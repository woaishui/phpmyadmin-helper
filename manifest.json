{
    "name": "phpmyadmin helper",
    "version": "1.0.0",
    "manifest_version": 2,
    "browser_action":
    {
        "default_icon":
        {
            "19": "icon.png",
            "38": "icon.png"
        },
        "default_title": "phpmyadmin helper",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],
    "options_ui":
    {
        "page": "options.html",
        "chrome_style": true
    }
}